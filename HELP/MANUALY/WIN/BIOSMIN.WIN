              DX = ni 栳st 桧tae asu (jako 0:046ch)
              AL = 0 pokud jet neuplynulo 24 hodin; jinak 1 (0:0470h)
              hodina    = stav_桧tae_asu / 65543 (1007H)
              zbytek1   = stav_桧tae_asu MOD 65543 (1007H)
              minuta    = zbytek1 / 1092 (0444H)
              zbytek2   = zbytek1 MOD 1092 (0444H)
              sekunda   = zbytek2 / 18.21
              zbytek3   = zbytek2 MOD 18.21
              setiny    = CINT (zbytek3 * 100)

INT   1AH/01H (AH =  1) - nastaven 桧tae asu systm. asovae
    vstup:    AH = 01H
              CX = vy 栳st 桧tae asu
              DX = ni 栳st 桧tae asu
         stav_桧tae_asu = (hodina * 65543.33) + (minuta * 1092.38) +
                            + (sekunda * 18.21) + (setina * .182)

